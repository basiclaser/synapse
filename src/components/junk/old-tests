/*ROUTE PAIR FOR TESTING LINKS*/
var testb = React.createClass({    
  render: function () {
    return <Link to="testa"> go to testa</Link>;
  }
});
var testa = React.createClass({    
  render: function () {
    return <Link to="testb"> go to testb</Link>;
  }
});
